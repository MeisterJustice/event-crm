<% include ../partials/admin-header %>

<section id="users" class="mt-5 container-fluid">
   <div class="row">
       <div class="col-lg-9">
           <div class="header">
               <h3>Users</h3>
           </div>
           <div class="table-responsive">

               <table class="table table-striped">
                   <thead class="bg-dark text-white">
                       <tr>
                           <th>#</th>
                           <th>First Name</th>
                           <th>Last Name</th>
                           <th>Email</th>
                           <th>Username</th>
                           <th>Sex</th>
                       </tr>
                   </thead>
                   <tbody>
                       
                       <% for(let i = users.length - 1; i >= 0; i--) { %>
                       
                       <tr>
                           <td scope="row"><%=i%></td>
                           <td><%=users[i].firstName%></td>
                           <td><%=users[i].lastName%></td>
                           <td><%=users[i].email%></td>
                           <td><%=users[i].username%></td>
                           <td><%=users[i].sex%></td>
                       </tr>
                       <%}%>
                               
                   </tbody>
               </table>
           </div>
       </div>
       <div class="col-lg-3">
            <div class="header">
                    <h3>Subscribers</h3>
                </div>
            <div class="table-responsive">

                    <table class="table table-striped">
                        <thead class="bg-dark text-white">
                            <tr>
                                <th>#</th>
                                <th>Email</th>
                                
                            </tr>
                        </thead>
                        <tbody>
                            
                            <% for(let i = emails.length - 1; i >= 0; i--) { %>
                            
                            <tr>
                                <td scope="row"><%=i%></td>
                                <td><%=emails[i].email%></td>
                            </tr>
                            <%}%>
                                    
                        </tbody>
                    </table>
                </div>
       </div>
   </div>
</section>

<% include ../partials/admin-footer %>